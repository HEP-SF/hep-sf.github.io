{
  "ignorePatterns": [
    {
      "pattern": "/assets/CWP-Charge-HSF.pdf"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/(.*)",
      "replacement": "/_site/$1"
    },
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://example.com"],
      "headers": {
        "Authorization": "Basic Zm9vOmJhcg==",
        "Foo": "Bar"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}

